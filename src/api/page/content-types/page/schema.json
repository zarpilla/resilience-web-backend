{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "Page",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        },
        "translate": {
          "translate": "translate"
        }
      },
      "type": "string",
      "required": true
    },
    "slug": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "metadata": {
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        },
        "translate": {
          "translate": "translate"
        }
      },
      "component": "meta.metadata"
    },
    "sections": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        },
        "translate": {
          "translate": "translate"
        }
      },
      "type": "dynamiczone",
      "components": [
        "sections.hero",
        "sections.columns",
        "sections.menu",
        "sections.slider",
        "sections.blurbs",
        "sections.bios",
        "sections.scroller",
        "sections.blog",
        "sections.capabilities",
        "sections.timeline",
        "sections.masonry",
        "sections.template",
        "sections.tabs"
      ]
    },
    "type": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "enumeration",
      "enum": [
        "page",
        "article",
        "resource",
        "scope",
        "project",
        "capability",
        "service"
      ],
      "default": "page"
    },
    "pageCss": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string"
    },
    "headerColorMode": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "light",
        "dark"
      ],
      "default": "light"
    },
    "tags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tag.tag",
      "pluginOptions": {
        "translate": {
          "translate": "translate"
        }
      }
    },
    "client": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        },
        "translate": {
          "translate": "translate"
        }
      },
      "type": "string"
    },
    "metaColor": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        },
        "translate": {
          "translate": "translate"
        }
      },
      "type": "string"
    },
    "readTime": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        },
        "translate": {
          "translate": "translate"
        }
      },
      "type": "string"
    },
    "scopes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::scope.scope",
      "pluginOptions": {
        "translate": {
          "translate": "translate"
        }
      }
    },
    "typology": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::typology.typology",
      "pluginOptions": {
        "translate": {
          "translate": "translate"
        }
      }
    },
    "year": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::year.year",
      "pluginOptions": {
        "translate": {
          "translate": "translate"
        }
      }
    },
    "author": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        },
        "translate": {
          "translate": "translate"
        }
      },
      "type": "string"
    }
