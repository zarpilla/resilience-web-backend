{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "Page",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true
    },
    "slug": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "metadata": {
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "meta.metadata"
    },
    "sections": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "dynamiczone",
      "components": [
        "sections.hero",
        "sections.image",
        "sections.columns",
        "sections.menu",
        "sections.slider",
        "sections.blurbs"
      ]
    },
    "type": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "enumeration",
      "enum": [
        "page",
        "article",
        "resource",
        "scope",
        "project"
      ],
      "default": "page"
    },
    "pageCss": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string"
    },
    "tag": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::tag.tag"
    },
    "headerColorMode": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "light",
        "dark"
      ],
      "default": "light"
    }
  }
}
